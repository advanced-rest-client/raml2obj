#%RAML 1.0 Library
uses:
  commonTypes: CommonTypes.raml
#  airPreferences: AirPreferences.raml
  simpleTypes: SimpleTypes.raml
  lists: Lists.raml
  commonPrefs: CommonPrefs.raml

types:
  Summary_AirOfferType:
    type: AirOfferType
    required: true
    properties:
      FlightSegmentRPH:
        description: "Applicable flight segment."
        type: simpleTypes.RPH_Type
        required: false
      ItineraryRPH:
        description: "Applicable itinerary."
        type: simpleTypes.RPH_Type
        required: false
      OrigDestRPH:
        description: "Applicable origin/ destination pair."
        type: simpleTypes.RPH_Type
        required: false
      TravelerRPH:
        description: "Applicable traveler."
        type: simpleTypes.RPH_Type
        required: false    
  Priced_AirPricedOfferType:
    type: AirPricedOfferType
    required: true
    properties:
      FlightSegmentRPH:
        description: "Applicable flight segment."
        type: simpleTypes.RPH_Type
        required: false
      ItineraryRPH:
        description: "Applicable itinerary."
        type: simpleTypes.RPH_Type
        required: false
      OrigDestRPH:
        description: "Applicable origin/ destination pair."
        type: simpleTypes.RPH_Type
        required: false
      TravelerRPH:
        description: "Applicable traveler."
        type: simpleTypes.RPH_Type
        required: false    
  Purchased_AirPurchasedOfferType:
    type: AirPurchasedOfferType
    required: true
    properties:
      FlightSegmentRPH:
        description: "Applicable flight segment."
        type: simpleTypes.RPH_Type
        required: false
      ItineraryRPH:
        description: "Applicable itinerary."
        type: simpleTypes.RPH_Type
        required: false
      OrigDestRPH:
        description: "Applicable origin/ destination pair."
        type: simpleTypes.RPH_Type
        required: false
      TravelerRPH:
        description: "Applicable traveler."
        type: simpleTypes.RPH_Type
        required: false    
  Summary:
    description: "Summary, non-priced air offers."
    type: array
    minItems: 0
    items: Summary_AirOfferType
  Priced:
    description: "Priced air offers."
    type: array
    minItems: 0
    items: Priced_AirPricedOfferType
  Purchased:
    description: "Air offers already purchased for a trip."
    type: array
    minItems: 0
    items: Purchased_AirPurchasedOfferType
  DefaultUsedInd:
    description: "If true, a system default rule was used."
    type: boolean
    required: false
  RuleInfo:
    description: "Information for individual airline applied rules."
    type: object
    required: false
    properties:
      Name:
        description: "The name of the applied rule."
        type: string
        required: true
      Version:
        description: "The version of the rule."
        type: integer
        required: false
  CheckAuthorization:
    description: "Specifies check information about the customer seeking authorization."
    type: commonTypes.CurrencyAmountGroup
    required: false
    properties:
      BankAcct:
        description: "Specifies bank account information about the customer seeking authorization."
        type: commonTypes.BankAcctType
        required: false
  CreditCardAuthorization:
    description: "Specifies credit card information about the customer seeking authorization."
    type: commonTypes.CurrencyAmountGroup
    required: false
    properties:
      CreditCard:
        description: "Specifies the credit card information for which authorization is required."
        type: commonTypes.PaymentCardType
        required: false
      ID:
        description: "Identification of an authorization party (e.g., merchant, acquirer)."
        type: commonTypes.UniqueID_Type[]
        minItems: 0
        maxItems: 5
      SourceType:
        description: "Information describing the point of sale."
        enum: ['NormalTransaction','MailOrPhoneOrder','UnattendedTerminal','MerchantIsSuspicious','eCommerceSecuredTransaction','eCommerceUnsecuredTransaction','InFlightAirPhone','CID_NotLegible','CID_NotOnCard']
        required: false
      ExtendedPaymentInd:
        description: "If true, the requester would like to apply extended payment conditions to this authorization."
        type: boolean
        required: false
      ExtendedPaymentQuantity:
        description: "The number of equal amount, partial payments for the extended payment."
        type: simpleTypes.Numeric1to999
        required: false
      ExtendedPaymentFrequency:
        description: "The frequency of extended payment installments."
        type: simpleTypes.TimeUnitType
        required: false
      AuthorizationCode:
        description: "This is the approval code received on the original authorization request. Only used in the case where the requested transaction is to reverse the authorization."
        type: string
        pattern: ^[A-Za-z0-9]{1,12}$
        required: false
      ReversalIndicator:
        description: "If true, indicates a request to reverse a credit authorization."
        type: boolean
        required: false
      CardPresentInd:
        description: "When true, the customer has actually presented the credit card."
        type: boolean
        required: false
      E_CommerceCode:
        description: "The electronic commerce indicator required for some credit card authorizations, such as the Verified by Visa Process."
        type: simpleTypes.AlphaNumericStringLength1to8
        required: false
      AuthTransactionID:
        description: "The authentication transaction identifier required for some credit card authorizations, such as the Verified by Visa Process."
        type: simpleTypes.StringLength1to32
        required: false
      AuthVerificationValue:
        description: "The cardholder authentication verfication value required for some credit card authorization, such as the Verified by Visa Process."
        type: simpleTypes.StringLength1to32
        required: false
  AccountAuthorization:
    description: "Specifies account information about the customer seeking authorization."
    type: commonTypes.CurrencyAmountGroup
    required: false
    properties:
      AccountInfo:
        description: "Information about the account."
        type: object
        required: false
        properties:
          AccountName:
            description: "The name on the account."
            type: simpleTypes.StringLength1to64
            required: false
          CompanyName:
            description: "Describes the name of the company to which the account is attached."
            type: simpleTypes.StringLength1to64
            required: false
          AccountID:
            description: "Identifier for the account assigned by the merchant."
            type: simpleTypes.StringLength1to64
            required: false
          Password:
            description: "Password attached to the account."
            type: simpleTypes.StringLength1to64
            required: false
          Code:
            description: "Describes the type of the account. Refer to OpenTravel Code List Payment Type (PMT)."
            type: simpleTypes.OTA_CodeType
            required: false
          NonISO_CurrencyCode:
            description: "Specifying the currency used in the authorization, when it isn't a ISO 4217, three alpha code (e.g., points, miles)."
            type: simpleTypes.StringLength1to8
            required: false
  CheckedInAirlineRPH:
    description: "The checked-in/lifted airline information."
    type: simpleTypes.RPH_Type
    required: false
  FlownAirlineSegmentRPH:
    description: "Reference to the flight segments for this coupon"
    type: simpleTypes.RPH_Type
    required: false
  OriginDestination:
    description: "Origin and destination information."
    type: commonTypes.OriginDestinationInformationType
    required: false
  AirItineraryDetail:
    description: "Detailed air itinerary information, including origin/destination options by flight segment."
    type: AirItineraryType
    required: false
  ActionCodeType:
    description: "Identifies the action code for a booking - OK, Waitlist etc."
    enum: ['OK','Waitlist','Other','Cancel','Need']
  AirSeatPreferenceType:
    description: "This type allows either a value from the OpenTravel Seat Preference Type (STP) or a string value."
    type: object
    required: false
    properties:
      Content:
        type: string
        required: false
      OTA_CodeType:
        type: simpleTypes.OTA_CodeType
        required: false
  AirTaxExemptionEnum:
    description: "Types of fees with tax exemption in certain locations."
    enum: ['PFC_PassengerFacilitationCharge','ZP_DomesticSegment','AY_SecurityFee','Other_']
  AncillaryServiceFamilyEnum:
    description: "Extensible service family types enumerated list."
    enum: ['All','A_FreeBaggageAllowance','C_BaggageCharges','F_FlightRelated','T_TicketRelated','M_MerchandiseRelated','R_RuleBuster','P_PrepaidBaggage','E_Embargos','GT_GroundTransportationNonAirServices','IE_InFlightEntertainment','LG_Lounge','Other_']
  DisplayOrderType:
    description: "An indication of how information should be ordered (sorted) in the response message."
    enum: ['ByDepartureTime','ByArrivalTime','ByJourneyTime','ByPriceHighToLow','ByPriceLowToHigh']
  Enum_SeatAvailability:
    description: "An extensible list of seat availability types."
    enum: ['AdvancedSeatSelectionSeat ','CourtesyReservedSeat','DownlineProtectedSeats','ExtraSeat','FreeSeat','GroupPreallocationSeat','NoSeatHere','ProtectedSeat','SeatAvailable','SeatBlocked_AirportUse','SeatBlocked_Deadload','SeatBlocked_LocalPassengerBoarded','SeatBlocked_OtherReason','SeatBlocked_ThroughPassengerBoarded','SeatHeld','SeatNotAvailableForInterlineThroughCheckin','SeatNotAvailableForPartnerAirlineUse ','SeatProtectedForCodeSharing','SeatOccupied','SeatOccupied_MedicalReasons','SeatReserved','SeatReserved_Generic','TransitPassengerOrLoad','UplineProtectedSeat ','Other_']
  Enum_SeatFeatures:
    description: "An extensible list of seat map features."
    enum: ['AdjacentSeat','AdjacentSeat_Aisle','AdjacentSeat_Together','Aisle','BlockedSeat','BlockedSeat_Permanent','Center','ExitRow','GroupSeating','InfantOnLap','NearCloset','NearLavatory','NearSpace','NearStairs','Nonsmoking','Overwing','Pet_Carriage','Pet_Medium','Pet_Small','PortableOxygen','PremiumSeat','PremiumSeat_Reassign','ProximitySeating_Together','PremiumSeat_Upgrade','Smoking','Specific','UnaccompaniedTravel_Minor','UnaccompaniedTravel_Senior','UnaccompaniedTravel_PassengerAssistance','Window','Other_']
  Enum_SeatMapOperations:
    description: "An extensible list of seat map operations."
    enum: ['GetSeatAssignment','GetSeatMap','GetSeatMapForPNR','GetSeatMapWithRules','GetSeatPurchases','GetSeatMapForSeatType','GetSeatZones','GetSeatMapForUpgrade','Other_']
  Enum_SeatRowCharacteristics:
    description: "An extensible list of seat row characteristics."
    enum: ['BufferRow','ExitLeft','ExitRight','ExitRow','ExitRowWithCabinFacilitiesInDesignatedColumn','ExitRowWithCabinFacilitiesInUndesignatedColumn','ExtraLegRoom','IndifferentRow','LowerdeckRow','MaindeckRow','NosmokingRow','NotOverwingRow','OverwingRow','RowDoesNotExist','RowWithCabinFacilitiesInDesignatedColumn','RowWithCabinFacilitiesInUndesignatedColumn','RowWithMovieScreen','SeatRestrictionsApply','SmokingRow','UpperDeckRow','Preferred_PreferentialRow','WingStart','WingEnd','Other_']
  Enum_UniqueIDType:
    description: "An extensible list of unique ID types."
    enum: ['Customer','CRO_CustomerReservationsOffice','CorporationRepresentative','Company','TravelAgency','Airline','Wholesaler','CarRental','Group','Hotel','TourOperator','CruiseLine','InternetBroker','Reservation','Cancellation','Reference','MeetingPlanningAgency','Other','InsuranceAgency','InsuranceAgent','Profile','ERSP_ElectronicReservationServiceProvider) ','ProvisionalReservation','TravelAgentPNR','AssociatedReservation','AssociatedItineraryReservation','AssociatedSharedReservation','Alliance','BookingAgent','Ticket','DividedReservation','Merchant','Acquirer','MasterReference','PurgedMasterReference','ParentReference','ChildReference','LinkedReference','Contract','ConfirmationNumber','FareQuote','ReissueRefundQuote','GroundTransportationSupplier','Other_']
  FareAmountType:
    description: "Used to specify if ticket amount is bulk, IT, or NOADC."
    enum: ['NOADC','Bulk','IT','Additional_Collection','Charter_Fare','No_Fare']
  FareApplicationType:
    description: "Indicates how the fare may be applied, such as one way or roundtrip."
    enum: ['OneWay','Return','HalfReturn','Roundtrip','OneWayOnly']
  FareStatusType:
    description: "Identifies whether the fare was constructed, published, created, etc."
    enum: ['constructed','published','created','fareByRule','fareByRulePrivate']
  GlobalIndicatorType:
    description: "Specifies the global travel area."
    enum: ['AP','AT','CT','DO','EH','FE','PA','PN','PO','RU','RW','SA','TS','WH']
  MealServiceType:
    description: "Enumerated List (Meal Code in brackets)- Breakfast (B); Snack (S); Dinner (D); Hot Meal (H); Lunch (L); Refreshments (R); Complimentary Liquor (C); Meal (M); Liquor for Purchase (P); Food for Purchase (F); Cold Meal (O); No Meal Service (-). Alternately, a String of Length 32 can be used if the above list does not suffice."
    enum: ['Breakfast','Snack','Dinner','Hot Meal','Lunch','Refreshments','Complimentary Liquor','Meal','Liquor for Purchase','Food for Purchase','Cold Meal','No Meal Service']
    type: simpleTypes.StringLength1to32
    required: false
  PricingSourceType:
    description: "It can be used to indicate whether the fare is public or private."
    enum: ['Published','Private','Both']
  PurposeType:
    description: "Used to specify base, net, sell or refund amount."
    enum: ['Sell','Net','Base','Refund','Additional']
  SpecialRemarkOptionType:
    description: "Identifies the type of special remark used."
    enum: ['Itinerary','Invoice','Endorsement','Save','Confidential','Free','GRMS','Split']
  StayUnitType:
    description: "Defines the 'Units' that can be applied to Stay restrictions."
    enum: ['Minutes','Hours','Days','Months','MON','TUES','WED','THU','FRI','SAT','SUN']
  AirDetailsRSAttributes:
    description: "Provides information concerning flight  times and mileage."
    type: object
    required: false
    properties:
      TotalFlightTime:
        description: "The total duration of time a flight is airborne."
        type: string
        pattern: -?P[0-9]+Y?([0-9]+M)?([0-9]+D)?(T([0-9]+H)?([0-9]+M)?([0-9]+(\.[0-9]+)?S)?)?
        required: false
      TotalGroundTime:
        description: "The total duration of time a flight is on the ground."
        type: string
        pattern: -?P[0-9]+Y?([0-9]+M)?([0-9]+D)?(T([0-9]+H)?([0-9]+M)?([0-9]+(\.[0-9]+)?S)?)?
        required: false
      TotalTripTime:
        description: "The total duration of time required for a flight operation (ground and air)."
        type: string
        pattern: -?P[0-9]+Y?([0-9]+M)?([0-9]+D)?(T([0-9]+H)?([0-9]+M)?([0-9]+(\.[0-9]+)?S)?)?
        required: false
      TotalMiles:
        description: "Total miles for a flight segment."
        type: integer
        minimum: 1
        required: false
  AirProcessingInfoGroup:
    description: "A collection of information that specifies how the message processing should occur or how the data should be returned."
    type: object
    required: false
    properties:
      TargetSource:
        description: "Specifies to whom the request should be targeted for the information that is to be returned."
        enum: ['Core','Vendor']
        required: false
      FlightSvcInfoIndicator:
        description: "If true, flight service information should be returned in the response."
        type: boolean
        required: false
      DisplayOrder:
        description: "Specifies the order in which the information should be returned."
        type: DisplayOrderType
        required: false
      ReducedDataIndicator:
        description: "If true, reduced data should be returned."
        type: boolean
        required: false
      BaseFaresOnlyIndicator:
        description: "If true, only base fare information should be returned"
        type: boolean
        required: false
      SearchType:
        description: "Specifies (at a high level) the type of search criteria for this request."
        enum: ['Normal','Window','ArrivalTime','PowerFlight','AvailableOnly','WaitlistOnly','AvailableAndWaitlist','FreighterFlights','Reward']
        required: false
      AvailabilityIndicator:
        description: "If true, booking class availability should be returned in the response for each of the flight segments."
        type: boolean
        required: false
  AirRowCharacteristicsGroup:
    description: "OpenTravel Codes for AirRowType"
    type: object
    required: false
    properties:
      AirRowType:
        description: "Refer to OpenTravel Code List Air Row Type (ROW)."
        type: simpleTypes.OTA_CodeType
        required: true
  BookingClassAvailabilityGroup:
    description: "Holds booking class and available seats quantity."
    type: object
    required: false
    properties:
      ResBookDesigCode:
        description: "Reservation Booking Designator (RBD) code (e.g. Y)."
        type: simpleTypes.UpperCaseAlphaLength1to2
        required: false
      ResBookDesigQuantity:
        description: "Seat quantity available for this Reservation Booking Designator (RBD)."
        type: simpleTypes.NumericStringLength1to3
        required: false
      ResBookDesigStatusCode:
        description: "Refer to OpenTravel Code List Res Book Designator Status Code (RBD). This provides the status (e.g. waitlist open, available, available by direct request to supplier only)."
        type: simpleTypes.OTA_CodeType
        required: false
  BookingClassPrefGroup:
    description: "Booking class code and preference level for specifying booking classes preferred/not preferred in a request."
    type: object
    required: false
    properties:
      ResBookDesigCode:
        description: "Booking class code"
        type: simpleTypes.UpperCaseAlphaLength1to2
        required: true
      PreferLevel:
        description: "Used to indicate a level of preference for an associated item."
        type: simpleTypes.PreferLevelType
        required: false
      ResBookDesigCodeType:
        description: "To specify the types of RBD's (Res Book Desig Codes) that should be returned as opposed to a specific RBD."
        enum: ['Displayable','NonDisplayable','All']
        required: false
  BookingClassPrefOptionalGroup:
    description: "Booking class code and preference level for specifying booking classes preferred/not preferred in a request with optional res designator."
    type: object
    required: false
    properties:
      ResBookDesigCode:
        description: "Booking class code"
        type: simpleTypes.UpperCaseAlphaLength1to2
        required: false
      ResBookDesigCodeType:
        description: "To specify the types of RBD's (Res Book Desig Codes) that should be returned as opposed to a specific RBD."
        enum: ['Displayable','NonDisplayable','All']
        required: false
      PreferLevel:
        description: "Used to indicate a level of preference for an associated item."
        type: simpleTypes.PreferLevelType
        required: false
  CodeListAirGroup:
    description: "Used to specify a code and its associated attributes; meaning is derived from actual use (plus SecondaryCode and SupplierCode)."
    type: object
    required: false
    properties:
      Code:
        description: "Any code used to specify an item, for example, type of traveler, service code, room amenity, etc."
        type: simpleTypes.StringLength1to8
        required: false
      CodeContext:
        description: "Identifies the source authority for the code."
        type: simpleTypes.StringLength1to32
        required: false
      Quantity:
        description: "Used to define the quantity for an associated element or attribute."
        type: integer
        minimum: 0
        required: false
      URI:
        description: "Identifies the location of the code table."
        type: string
        pattern: \w+:(\/?\/?)[^\s]+
        required: false
      SecondaryCode:
        description: "An additional attribute to allow flexibility for particular organizations who require an additional code."
        type: simpleTypes.StringLength1to16
        required: false
      SupplierCode:
        description: "An additional attribute to allow flexibility for particular organizations who require an additional supplier code."
        type: simpleTypes.UpperCaseAlphaLength1to3
        required: false
  DirectAndStopsGroup:
    description: "Attribute collection providing information on direct flight categorization and the number of stops made."
    type: object
    required: false
    properties:
      DirectFlightsOnly:
        description: "Indicates the same flight number on the same airline regardless of number of stops in most cases. "
        type: boolean
        required: false
      NumberStops:
        description: "Information regarding the number of stops made."
        type: integer
        minimum: 0
        maximum: 9
        required: false
  DiscountPricingGroup:
    description: "The information needed for applying a discount to a fare."
    type: object
    required: false
    properties:
      Purpose:
        description: "Used to specify the purpose of the discount pricing."
        enum: ['Airline','Custom']
        required: false
      Type:
        description: "Specifies the type of amount being sent."
        enum: ['Amount','Percent','PlusUpAmount']
        required: false
      Usage:
        description: "Used to specify how the discount is to be applied."
        enum: ['Plus','Minus']
        required: false
      Discount:
        description: "The monetary amount or percentage of discount that should be applied."
        type: simpleTypes.MoneyOrPercentageType
        required: false
      TicketDesignatorCode:
        description: "Specifies the code applicable to the fare that is being discounted."
        type: simpleTypes.StringLength1to16
        required: false
      Text:
        type: simpleTypes.StringLength1to128
        required: false
  FareRestrictPrefGroup:
    description: "Identifies preferences for airfare restrictions acceptable or not acceptable for a given travel situation."
    type: object
    required: false
    properties:
      PreferLevel:
        description: "Used to indicate a level of preference for an associated item."
        type: simpleTypes.PreferLevelType
        required: false
      FareRestriction:
        description: "Identifies the type of fare restriction, such as None, Advance Purchase and Change Penalties. Refer to OpenTravel Code List Fare Restriction (FAR)."
        type: simpleTypes.OTA_CodeType
        required: false
      Date:
        description: "A date that is associated to the fare restriction."
        type: simpleTypes.DateOrDateTimeType
        required: false
  FareTypePrefGroup:
    description: "Specifies a type of fare and a preference level for the type."
    type: object
    required: false
    properties:
      FareType:
        description: "The type of fare required (e.g. unrestricted, excursion). Refer to the Fare Qualifier OpenTravel Code list (FAQ)."
        type: object
        required: true
        properties:
          OTA_CodeType:
            type: simpleTypes.OTA_CodeType
            required: false
          Content:
            type: simpleTypes.UpperCaseAlphaLength1to3
            required: false
      PreferLevel:
        description: "Used to indicate a level of preference for a fare type."
        type: simpleTypes.PreferLevelType
        required: false
  FlifoLegAttributes:
    description: "Provides information for a flight leg."
    type: object
    required: false
    properties:
      FlightNumber:
        description: "The flight number of the flight."
        type: simpleTypes.FlightNumberType
        required: false
      JourneyDuration:
        description: "The duration of the flight from departure location to destination  location."
        type: string
        pattern: -?P[0-9]+Y?([0-9]+M)?([0-9]+D)?(T([0-9]+H)?([0-9]+M)?([0-9]+(\.[0-9]+)?S)?)?
        required: false
      GroundDuration:
        description: "The duration of a ground stop."
        type: string
        pattern: -?P[0-9]+Y?([0-9]+M)?([0-9]+D)?(T([0-9]+H)?([0-9]+M)?([0-9]+(\.[0-9]+)?S)?)?
        required: false
      AccumulatedDuration:
        description: "The total duration time of the flight. This is the combination of both JourneyDuration and GroundDuration."
        type: string
        pattern: -?P[0-9]+Y?([0-9]+M)?([0-9]+D)?(T([0-9]+H)?([0-9]+M)?([0-9]+(\.[0-9]+)?S)?)?
        required: false
      LegDistance:
        description: "Miles aquired per flight segments, usually used for earning of frequent flyer miles."
        type: integer
        minimum: 0
        required: false
  FlightRefNumberGroup:
    description: "A reference place holder used as a pointer to link back to a flight number."
    type: object
    required: false
    properties:
      RPH:
        description: "A reference pointer used to link a flight number to the search or response."
        type: simpleTypes.RPH_Type
        required: false
  GlobalDirectionGroup:
    description: "The global direction and maximum permitted miles for the fare."
    type: object
    required: false
    properties:
      GlobalIndicatorCode:
        description: "Indicates the global direction."
        type: GlobalIndicatorType
        required: false
      MaximumPermittedMileage:
        description: "The maximum mileage (in miles) that can be travelled under this contract."
        type: integer
        required: false
      IncludeIndicator:
        description: "When true, the global direction can be used for travel. When false, the global direction cannot be used for travel."
        type: boolean
        required: false
  NegotiatedFareAttributes:
    description: "Attribute collection providing negotiated fare profile."
    type: object
    required: false
    properties:
      NegotiatedFareInd:
        description: "If true, this is a negotiated fare."
        type: boolean
        required: false
      NegotiatedFareCode:
        description: "A code used to identify the negotiated fare."
        type: string
        required: false
      ATPCO_NegCategoryCode:
        description: "The ATPCO category code for this negotiated fare."
        type: string
        required: false
  OperationTimeGroup:
    description: "Provides information for operational events for a flight leg (e.g., off-ground)"
    type: object
    required: false
    properties:
      Time:
        description: "Time or date/time an operational event happened."
        type: simpleTypes.TimeOrDateTimeType
        required: true
      OperationType:
        description: "Type of operational event (e.g., off-ground). References the OpenTravel Code Table Other Time Type (OTT)."
        type: simpleTypes.OTA_CodeType
        required: true
      TimeType:
        description: "Describes whether the operational event time is scheduled, estimated or actual."
        enum: ['Actual','Scheduled','Estimated']
        required: true
      ReasonCode:
        description: "The reason attributed to a delay or cancellation. Refers to OpenTravel Code Table Flight Delay Code (FDC)."
        type: simpleTypes.OTA_CodeType
        required: false
  OriginDestinationGroup:
    description: "Used to specify the origin and destination airport/city codes."
    type: object
    required: false
    properties:
      OriginCityCode:
        description: "The origin airport city code."
        type: simpleTypes.StringLength1to8
        required: false
      OriginCodeContext:
        description: "Identifies the context of the origin city code such as IATA, ARC, or internal code."
        type: simpleTypes.StringLength1to32
        required: false
      DestinationCityCode:
        description: "The destination airport city code."
        type: simpleTypes.StringLength1to8
        required: false
      DestinationCodeContext:
        description: "Identifies the context of the destination city code such as IATA, ARC, or internal code."
        type: simpleTypes.StringLength1to32
        required: false
  PriceRequestAttributes:
    description: "Attribute collection used to describe a price request."
    type: object
    required: false
    properties:
      FareQualifier:
        description: "Used to specify the type of fare required. Refer to OpenTravel Code List Fare Qualifier (FAQ) or use airline industry standard fare codes."
        type: object
        required: false
        properties:
          Content:
            type: simpleTypes.UpperCaseAlphaLength1to3
            required: false
          OTA_CodeType:
            type: simpleTypes.OTA_CodeType
            required: false
      NegotiatedFaresOnly:
        description: "Indicator to identify whether or not the price is applicable only to private fares."
