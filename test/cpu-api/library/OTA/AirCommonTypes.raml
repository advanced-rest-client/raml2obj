        type: string
        required: false
      Description:
        description: "A description of the service family."
        type: string
        required: false
      RPH:
        description: "A unique ID for this service family that is referenced elsewhere in this schema."
        type: simpleTypes.RPH_Type
        required: false
      ResultsAction:
        description: "An indicator if the service code is required and/or should be included or excluded from the offer results."
        type: simpleTypes.IncludeExcludeType
        required: false
