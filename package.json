  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "3.11.x",
    "eslint-config-loopwerk": "6.0.x",
    "glob": "7.1.x",
    "mocha": "3.2.x",
    "uglify-js": "^2.7.5"
  },
  "homepage": "https://github.com/raml2html/raml2obj",
  "keywords": [
