    "browser": "./node_modules/.bin/browserify browser.js -o build/browserified.js && ./node_modules/.bin/babel build/browserified.js > build/babeled.js && ./node_modules/.bin/uglifyjs --screw-ie8 build/babeled.js > build/raml2object.js"
