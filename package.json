    "type": "git",
    "url": "git://github.com/raml2html/raml2obj.git"
  },
  "preferGlobal": false,
  "scripts": {
    "json": "node test/output-json.js",
    "lint": "eslint ./",
    "lintfix": "eslint --fix ./",
